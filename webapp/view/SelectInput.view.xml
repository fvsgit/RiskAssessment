<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="riskassessment.controller.SelectInput"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="page" title="{AppModel>/SelectInput/Title}" showNavButton="true" navButtonPress="onNavBack">
		<content>
			<Toolbar>
				<SearchField liveChange="onSearch" width="100%"/>
			</Toolbar>
			<List id="lstList" selectionChange="onSelectionChange" mode="SingleSelectMaster" includeItemInSelection="true"> 
			</List>
		</content>
	</Page>
</mvc:View>